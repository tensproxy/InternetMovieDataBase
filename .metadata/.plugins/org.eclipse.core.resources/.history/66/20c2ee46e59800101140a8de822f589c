	public RepConTXT () {
		super("txt");
	}
	@Override
	public String convert(EntryList<Entry> inEntries) {
		StringBuilder aSB = new StringBuilder();
		for (Entry e : inEntries) {
			aSB.append(this.toString(e));
		}
		return null;
	}
	

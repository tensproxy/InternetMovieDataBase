package layer3;

public class Thingy {

	public static String convert(EntryList<Entry> inList, String string) {
		StringBuilder aSB = new StringBuilder();
		for ( Entry e : inList ){
			
		}
		return aSB.toString();
	}
	
	public String toTXT(Entry e){
		StringBuilder aSB = new StringBuilder();
		aSB.append(e.getID() + "." + e.getLanguage() + System.getProperty("line.separator"));
		aSB.append(e.lgetTitle() + ":")
		aSB.append(e.lGetActors() + ": " + e.getActors() );
		aSB.append(e)
		return aSB.toString();
	}

}

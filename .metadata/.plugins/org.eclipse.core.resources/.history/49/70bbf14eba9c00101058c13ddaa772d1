		Entry e1 = new Entry("tt1000001", "de");
		Entry e2 = new Entry("tt1000002", "de");
		Entry e3 = new Entry("tt1000003", "de");
		Entry e4 = new Entry("tt1000004", "de");
		
		DataBase db = new DataBase();
		
		int a = db.newList();

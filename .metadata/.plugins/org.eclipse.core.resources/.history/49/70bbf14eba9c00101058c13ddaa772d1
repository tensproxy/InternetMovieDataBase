		db.addEntry(a, e1);
		db.addEntry(a, e2);
		db.addEntry(a, e3);
		db.addEntry(a, e4);
		
		int b = db.newList();
		db.addEntry(b, e0);
		db.addEntry(b, e1);
		db.addEntry(b, e2);
		db.addEntry(b, e3);
		db.addEntry(b, e4);

//		System.out.println(db.toString(a));
